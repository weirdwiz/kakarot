{
  "name": "kakarot-monorepo",
  "version": "0.1.0",
  "description": "Kakarot monorepo: Desktop app + backend API",
  "main": "dist/main/index.js",
  "private": true,
  "workspaces": [
    ".",
    "treeto-backend"
  ],
  "scripts": {
    "install:all": "npm install && npm install --workspace treeto-backend",
    "dev": "vite",
    "dev:app": "vite",
    "dev:backend": "npm --workspace treeto-backend run dev",
    "dev:all": "concurrently \"npm run dev:app\" \"npm run dev:backend\"",
    "build": "tsc && vite build && electron-builder",
    "build:app": "tsc && vite build && electron-builder",
    "build:backend": "npm --workspace treeto-backend run build",
    "build:vite": "vite build",
    "start": "electron .",
    "dev:electron": "vite",
    "test:prep": "tsx src/main/test-prep.ts",
    "lint": "eslint src",
    "test:prep:mock": "tsx src/main/test-prep-mock.ts",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@deepgram/sdk": "^4.11.3",
    "@hubspot/api-client": "^13.4.0",
    "@types/jsforce": "^1.11.6",
    "assemblyai": "^4.8.0",
    "audiotee": "^0.0.7",
    "bindings": "^1.5.0",
    "chrono-node": "^2.9.0",
    "dotenv": "^17.2.3",
    "jsforce": "^3.10.13",
    "lucide-react": "^0.562.0",
    "openai": "^4.104.0",
    "sql.js": "^1.11.0",
    "uuid": "^11.0.0",
    "zustand": "^5.0.9"
  },
  "devDependencies": {
    "@eslint/js": "^9.16.0",
    "@types/node": "^22.10.0",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "@types/sql.js": "^1.4.9",
    "@types/uuid": "^10.0.0",
    "@types/ws": "^8.18.1",
    "@vitejs/plugin-react": "^4.3.0",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.1.0",
    "electron": "^33.2.0",
    "electron-builder": "^25.1.0",
    "eslint": "^9.16.0",
    "eslint-plugin-react": "^7.37.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "postcss": "^8.4.49",
    "react": "^18.3.0",
    "react-dom": "^18.3.0",
    "tailwindcss": "^3.4.0",
    "tsx": "^4.21.0",
    "typescript": "^5.7.0",
    "typescript-eslint": "^8.18.0",
    "vite": "^6.0.0",
    "vite-plugin-electron": "^0.28.0",
    "vite-plugin-electron-renderer": "^0.14.0",
    "wait-on": "^8.0.0"
  },
  "author": "",
  "license": "MIT"
}
